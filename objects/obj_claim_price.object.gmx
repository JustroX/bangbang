{
    var list =global.bag[| i];
    if(list[| item_quantity]&lt;=0)
    {
        ds_list_destroy(list);
        ds_list_delete(global.bag,i);
    }
}


//save settings
save_player_stats();

with(obj_winner_control) {instance_destroy();}

room_goto(room_multi_player);

</string>
