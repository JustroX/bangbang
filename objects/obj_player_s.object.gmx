            <string>///define variables
image_speed = 0;
image_index = 1;

//for movements
self.pressed_jump = false;
self.pressed_left = false;
self.pressed_right = false;
self.pressed_down = false;

//define name
self.name = global.username;

//set sprite of feet
feet = spr_walk_animation;


//define appearance
self.mask = global.mask;
self.jetpack = global.jetpack;
self.helmet = global.helmet;
self.vest = global.vest;
self.pants = global.pants;
self.shoes = global.shoes;
self.loaded = true;

//define statistics
self.hp = global.hp;
self.jump = global.jump;
self.jet_fuel = global.jet_fuel;
self.player_speed = global.player_speed;
self.hp_regen  = global.hp_regen;
self.armor     = global.armor;
self.crit_chance = global.crit_chance;
self.scaling    = 1;

//define in game items array
self.gun_choices = ds_list_create();
self.consumables = ds_list_create();

///load guns or consumables

for(var i=0; i&lt;ds_list_size(global.bag); i++)   //for all items in the bag
{
    var bag_item = global.bag[| i];                     //get the item
    if(db_is_gun(obj_database,bag_item[| item_id]))     //if item is gun
    {
        var gun_choices_size = ds_list_size(self.gun_choices);          //get size of gun choices array
        var gmap = db_get(obj_database,"gun",db_get_ingame(obj_database,bag_item[| item_id]));  //get object map from database
        var temp_list = ds_list_create();                               //create temporary list
        temp_list[| item_id] = bag_item[| item_id];                         //save details from database to temp list
        temp_list[| 1 ]      = gmap[? "ammo_count"];
        // show_message(bag_item[| item_id]);
        self.gun_choices[| gun_choices_size] = temp_list ;              //add new value in gun choices
        ds_list_mark_as_list(self.gun_choices,gun_choices_size);        //mark item in list as list
    }
    else
    if(db_is_consumable(obj_database,bag_item[| item_id]))              //if item is consumable
    {
        var consumables_size = ds_list_size(self.consumables);          //get size of consumable array
        self.consumables[| consumables_size] = bag_item;                //add new consumable in consumable array
        ds_list_mark_as_list(consumables,consumables_size);             //mark new value as list
    } 
}

//choose gun
gun_choice = 0;                                                         //set gun choice to 0
if(ds_list_size(gun_choices)!=0)                                        //if list is not empty
{
    var tgun = gun_choices[| gun_choice];                               //get gun info
    self.gun = tgun[| item_id];                                         //save item id of gun in self.gun
}
else
{
    self.gun = -1;                                                      // if empty set gun to none
}

//choose consumable
consumable_choice = 0;                                                  //set consumble choice to 0
if(ds_list_size(consumables)!=0)                                        //if consumable list is empty
{
    var tconsumable = consumables[| consumable_choice];                 //get consumable 
    self.consumable = tconsumable[| item_id];                           //save item id of consumable
}
else
{                                                                       //if empty
    self.consumable = -1;                                               //set consumable to -1
}
      
self.pre_loaded = false;                                                //[FOR NETWORKING]
self.pre_gun    = -1;                                                   //[FOR NETWORKING]

//define id of appearance and variables

gun_id = -1;
mask_id =-1;
jetpack_id =-1;
helmet_id =-1;
vest_id =-1;
pants_id =-1;
shoes_id =-1;
aura_id =-1;

//define arm angle
theta = 0;

///define gun variables
gun_callback = -1;
gun_ammo = 0;

//define prev jump
prev_jump = 0;

//define max hp
max_hp = -1;

//define platforming variables
touch_ladder = false;
pressed_prev = false;

//define if dead
dead = false;
</string>
